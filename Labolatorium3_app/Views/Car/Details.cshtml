}

<div class="mb-3">
    <label asp-for="Model">Model:</label>
    <strong>@Model.Model</strong>
</div>

<div class="mb-3">
    <label asp-for="Manufacturer">Producent:</label>
    <strong>@Model.Manufacturer</strong>
